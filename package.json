{
    "name": "bulk-vouchers-creation",
    "version": "1.0.0",
    "description": "Bulk vouchers creation app with express, postgresql",
    "main": "app.js",
    "scripts": {
        "test": "NODE_ENV=test mocha",
        "db-create": "node_modules/.bin/sequelize db:create",
        "db-drop": "node_modules/.bin/sequelize db:drop",
        "migrate": "node_modules/.bin/sequelize db:migrate",
        "migrate-reset": "node_modules/.bin/sequelize db:migrate:undo:all",
        "seed": "node_modules/.bin/sequelize db:seed:all",
        "seed-reset": "node_modules/.bin/sequelize db:seed:undo:all",
        "start": "node app.js",
        "dev": "nodemon app.js"
    },
    "author": "Waseem Ansar",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.18.3",
        "casual": "^1.6.0",
        "chai": "^4.2.0",
        "express": "^4.16.4",
        "lodash": "^4.17.15",
        "mocha": "^6.2.2",
        "moment": "^2.24.0",
        "multer": "^1.4.1",
        "pg": "^7.9.0",
        "pg-hstore": "^2.3.2",
        "sequelize": "^5.21.2",
        "sequelize-cli": "^5.5.1",
        "xlsx": "^0.14.2"
    },
    "devDependencies": {
        "nodemon": "^1.18.11"
    }
}
